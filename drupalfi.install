<?php
/**
 * @file
 * Install, update and uninstall functions for the drupalfi installation profile.
 */

/**
 * Implements hook_install().
 *
 * Performs actions to set up the site for this profile.
 *
 * @see system_install()
 */
function drupalfi_install() {
  $default_theme = variable_set('theme_default', 'drupalfi');
}
